<h1>All Reviews</h1>
<% @reviews.each do |review| %>
  <div class="review">
    <p><strong>Review for trip to: </strong><%= review.trip.destination.name %></p> 
    <p><strong>Rating: </strong><%= review.rating %> ‚≠ê</p>
    <p><strong>Comment: </strong><%= review.comment %></p>
    <p>
        <strong>User: </strong><%= "#{review.user.first_name} #{review.user.last_name}" %></p>
  </div>
<% end %>

